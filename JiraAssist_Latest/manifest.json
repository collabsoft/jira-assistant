{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "A Jira Assistant Update to Manifest V3",
  "short_name": "Jira ++",
  "version": "0.1.0",
  "description": "Help to track & upload work done on a Jira Ticket on timely basis and generate report.",
  "icons": {
    "24": "/assets/icon_24.png",
    "32": "/assets/icon_32.png",
    "48": "/assets/icon_48.png"
  },
  "browser_action": {
    "default_icon": {
      "24": "/assets/icon_24.png",
      "32": "/assets/icon_32.png",
      "48": "/assets/icon_48.png"
    },
    "default_title": "Jira Assistant",
    "default_popup": "/menu.html"
  },
  "commands": {
    "open-ja-dashboard": {
      "suggested_key": {
        "default": "Alt+J",
        "mac": "Command+Shift+J"
      },
      "description": "Open JiraAssist dashboard page"
    }
  },
  "oauth2": {
    "client_id": "?",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "content_security_policy": "script-src 'self' ; object-src 'self';",
  "offline_enabled": true,
  "permissions": [
    "storage"
  ],
  "optional_permissions": [
    "unlimitedStorage",
    "identity",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "author": "DaBish - new",
  "key": ""
}
